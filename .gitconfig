[user]
	name = Doug Knesek
	email = doug.knesek@gmail.com
[alias]
	l = log --graph --pretty=format:'%Creset%C(bold blue)%an %Creset%C(bold yellow)%d %C(bold green)%cr %Creset %Cred%H\n%s %Creset' --abbrev-commit --date=relative
        l2 = log --graph --pretty=format:'%Cred%h%Creset -%C(yellow)%d%Creset %s %Cgreen(%cr) %C(bold blue)<%an>%Creset' --abbrev-commit

[color]
	ui = auto
[color "branch"]
	current = yellow reverse
	local = yellow
	remote = green
[color "diff"]
	meta = yellow bold
	frag = magenta bold
	old = red bold
	new = green bold
[color "status"]
	added = green
	changed = yellow
	untracked = cyan
	deleted = red
[help]
	autocorrect = 1
[push]
	default = tracking